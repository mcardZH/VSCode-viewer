{
  "name": "msv",
  "displayName": "Molecular Simulation Visualizer",
  "description": "Provides visualization support for molecular structures and trajectories from Gromacs and other molecular simulation software, facilitating research in molecular dynamics and structural biology. Adapted from: ArianJamasb's molstar/VSCoding-Sequence.",
  "version": "0.0.1",
  "publisher": "mcardZH",
  "author": {
    "name": "mcardZH"
  },
  "icon": "img/logo.png",
  "engines": {
    "vscode": "^1.62.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcardZH/VSCode-viewer"
  },
  "license": "MIT",
  "categories": [
    "Other",
    "Visualization",
    "Data Science"
  ],
  "keywords": [
    "gromacs",
    "molecular dynamics",
    "simulation",
    "trajectory",
    "visualization",
    "msv",
    "protein",
    "molecule",
    "biology",
    "3d",
    "structure",
    "pdb",
    "pdbqt",
    "mmcif",
    "sdf",
    "ent",
    "mol",
    "mol2",
    "xyz",
    "cif",
    "gro",
    "xtc",
    "trr",
    "macromolecule",
    "bioinformatics",
    "computational biology",
    "chemistry",
    "cheminformatics",
    "computational chemistry",
    "分子模拟",
    "分子动力学",
    "轨迹",
    "结构生物学"
  ],
  "activationEvents": [
    "onCommand:protein-viewer.start",
    "onCommand:protein-viewer.activateFromFiles",
    "onCommand:protein-viewer.activateFromFolder",
    "onCommand:protein-viewer.ESMFold"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "protein-viewer.start",
        "title": "Start Protein Viewer"
      },
      {
        "command": "protein-viewer.activateFromFiles",
        "title": "Launch Protein Viewer from File(s)"
      },
      {
        "command": "protein-viewer.activateFromFolder",
        "title": "Launch Protein Viewer from Folder"
      },
      {
        "command": "protein-viewer.ESMFold",
        "title": "ESMFold Sequence"
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "when": "resourceExtname == .pdb || resourceExtname == .pdb.gz || resourceExtname == .PDB || resourceExtname == .mol2 || resourceExtname == .MOL2 || resourceExtname == .sdf || resourceExtname == .SDF || resourceExtname == .mmCIF || resourceExtname == .mmcif || resourceExtname == .cif.gz || resourceExtname == .MMCIF || resourceExtname == .Mol || resourceExtname == .mol || resourceExtname == .MOL || resourceExtname == .xyz || resourceExtname == .XYZ || resourceExtname == .ent || resourceExtname == .ENT || resourceExtname == .pdbqt || resourceExtname == .PDBQT || resourceExtname == .cif || resourceExtname == .CIF || resourceExtname == .mcif || resourceExtname == .MCIF || resourceExtname == .gro || resourceExtname == .GRO",
          "command": "protein-viewer.activateFromFiles",
          "group": "navigation"
        },
        {
          "when": "explorerResourceIsFolder",
          "command": "protein-viewer.activateFromFolder",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "molstar": "^4.8.0",
    "node-fetch": "^2.6.1",
    "vscode-uri": "^3.0.8",
    "yarn": "^1.22.22"
  }
}
